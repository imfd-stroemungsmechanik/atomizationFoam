FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;

    nu              1e-06;

    rho             1000;
}

air
{
    transportModel  Newtonian;

    nu              15e-06;

    rho             1;
}

sigma   0.04;


